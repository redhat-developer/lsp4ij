{
  "id": "vscode-js-debug",
  "name": "VSCode JS Debug",
  "launch": {
    "default": "node <<insert base directory>>/js-debug/src/dapDebugServer.js ${port} 127.0.0.1"
  },
  "debugServerReadyPattern": "Debug server listening at ",
  "attach": {
    "port": "$port"
  },
  "fileTypeMappings": [
    {
      "fileType": {
        "name": "JavaScript"
      },
      "languageId": "javascript"
    },
    {
      "fileType": {
        "name": "JavaScript-React",
        "patterns": [
          "*.jsx"
        ]
      },
      "languageId": "javascriptreact"
    },
    {
      "fileType": {
        "name": "TypeScript",
        "patterns": [
          "*.ts"
        ]
      },
      "languageId": "typescript"
    },
    {
      "fileType": {
        "name": "TypeScript-React",
        "patterns": [
          "*.tsx"
        ]
      },
      "languageId": "typescriptreact"
    }
  ]
}