{
  "id": "julia",
  "name": "Julia",
  "launch": {
    "default": "julia $PROJECT_DIR$/server.jl ${port}"
  },
  "debugServerReadyPattern": "Listening on port ${port}",
  "attach": {
    "port": "$port"
  },
  "fileTypeMappings": [
    {
      "fileType": {
        "name": "Julia",
        "patterns": [
          "*.jl"
        ]
      },
      "languageId": "julia"
    }
  ]
}