{
  "id": "camel-lsp-server",
  "name": "Camel Language Server",
  "programArgs": {
    "default": "java -jar camel-lsp-server-XXX.jar"
  },
  "languageMappings": [
    {
      "language": "JAVA",
      "languageId": "java"
    }
  ],
  "fileTypeMappings": [
    {
      "fileType": {
        "name": "XML",
        "patterns": [
          "*.xml"
        ]
      },
      "languageId": "xml"
    },
    {
      "fileType": {
        "name": "JAVA",
        "patterns": [
          "*.java"
        ]
      },
      "languageId": "java"
    },
    {
      "fileType": {
        "name": "Properties",
        "patterns": [
          "*.properties"
        ]
      },
      "languageId": "properties"
    },
    {
      "fileType": {
        "name": "Spring Boot Properties",
        "patterns": [
          "application.properties"
        ]
      },
      "languageId": "spring-boot-properties"
    },
    {
      "fileType": {
        "name": "YAML",
        "patterns": [
          "*.yaml",
          "*.yml"
        ]
      },
      "languageId": "yaml"
    }
  ]
}