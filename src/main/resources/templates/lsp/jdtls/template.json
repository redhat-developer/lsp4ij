{
  "id": "jdt-ls",
  "name": "JDT Language Server",
  "dev": true,
  "programArgs": {
    "windows": "\"${BASE_DIR}/bin/jdtls.bat\" -configuration \"C:/Users/${user.name}/.cache/jdtls\" -data \"$DATA_DIR\"",
    "default": "\"${BASE_DIR}/bin/jdtls\" -configuration \"~/.cache/jdtls\" -data \"$DATA_DIR\""
  },
  "languageMappings": [
    {
      "language": "JAVA",
      "languageId": "java"
    }
  ],
  "fileTypeMappings": [
    {
      "fileType": {
        "name": "JAVA",
        "patterns": [
          "*.java"
        ]
      },
      "languageId": "java"
    }
  ]
}